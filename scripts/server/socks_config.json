{
    "log": {
        "access": "/var/log/v2ray/access.log",
        "error": "/var/log/v2ray/error.log",
        "loglevel": "warning"
    },

    "inbounds": [{
        "port": #SOCKS_PORT#,
        "protocol": "socks",
        "settings": {
            "auth": "noauth",
            "udp": false
        }
    }],

    "outbounds": [{
        "protocol": "freedom",
        "settings": {}
    }]
}